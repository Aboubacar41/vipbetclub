    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
    <script>
        // Gestion des cookies
        function acceptCookies() {
            document.getElementById('cookieBanner').style.display = 'none';
            localStorage.setItem('cookiesAccepted', 'true');
        }

        window.onload = function() {
            if (!localStorage.getItem('cookiesAccepted')) {
                document.getElementById('cookieBanner').style.display = 'block';
            }
        };

        // Confirmation de paiement
        function confirmPayment(method) {
            alert(`Paiement effectué via ${method} ! Merci de vérifier votre confirmation.`);
        }

        // Initialisation du carrousel Swiper
        const swiper = new Swiper('.swiper-container', {
            loop: true,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
            navigation: {
                nextEl: '.swiper-button-next',
                prevEl: '.swiper-button-prev',
            },
        });

        // Validation du formulaire de contact
        document.querySelector('.contact-form').addEventListener('submit', function(e) {
            e.preventDefault();
            const name = this.querySelector('input[type="text"]').value;
            const email = this.querySelector('input[type="email"]').value;
            const message = this.querySelector('textarea').value;

            if (name && email && message) {
                if (/^\S+@\S+\.\S+$/.test(email)) {
                    alert('Message envoyé avec succès ! Je vous répondrai bientôt.');
                    this.reset();
                } else {
                    alert('Veuillez entrer un email valide.');
                }
            } else {
                alert('Veuillez remplir tous les champs.');
            }
        });

        // Balises méta dynamiques
        document.title = "Haute Connaissance Spirituelle : Éveillez votre potentiel divin avec Goame-Bila";
        document.querySelector('meta[name="description"]').setAttribute("content", "Haute Connaissance Spirituelle : Éveillez votre âme avec Goame-Bila, féticheur traditionnel. Consultations géomantiques, talismans sacrés, méditations puissantes, et solutions spirituelles pour tous vos problèmes.");
    </script>

